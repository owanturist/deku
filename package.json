{
  "name": "deku",
  "version": "2.0.0-rc16",
  "license": "MIT",
  "repository": "dekujs/deku",
  "description": "Render interfaces using pure functions and virtual DOM",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:test": "tsc -p tsconfig.test.json",
    "lint": "tslint \"src/**/**.ts*\"",
    "pretest": "npm run clean && npm run build:test",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "awesome-typescript-loader": "^3.0.0-beta.14",
    "tslint": "^5.1.0",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "dift": "^0.1.12"
  },
  "keywords": [
    "deku",
    "functional",
    "react",
    "virtual",
    "dom",
    "elm",
    "redux"
  ],
  "ava": {
    "files": [
      "dist/**/*.spec.js"
    ],
    "source": [
      "dist/**/*.js",
      "!dist/**/*.spec.js"
    ]
  }
}
