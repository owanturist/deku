{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-namespace": "allow-decalarations",
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "no-var-requires": false,
    "no-unused-variable": true,
    "trailing-comma": [
      true
    ],
    "ordered-imports": [
      false
    ]
  }
}
